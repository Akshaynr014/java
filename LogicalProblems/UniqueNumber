public class UniqueElementFinder {
    public static void main(String[] args) {
        int[] arr = {2, 3, 5, 4, 5, 3, 4};
        int unique = 0;

        for (int num : arr) {
            unique ^= num; // XOR operation
        }

        System.out.println("The unique element is: " + unique);
    }
}
